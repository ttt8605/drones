     <% let x = 1; %>
     <section class="visibleImages">
         <% for (let project of projects) { %>
             <% if (x < 7) { %>
                 <% if (x != 3) { %>
                     <% if (x % 2 === 1) { %>
                         <div class="twoImgRow">
                     <% } %>
                     
                     <div class="img2">
                      <a class="imagesLink" href="/projects/<%= project._id %>">
                        <img class="picture" src="<%=project.images[0].url %>" alt="" loading="lazy">
                        <div class="textAndButton">
                         <h1 class="img2Text"><%= project.name %></h1>
                         <button class="img2Button">&rarr;</button>
                         </div>
                        </a>
                     </div>
                     <% if (x % 2 === 0) { %>
                         </div> <!-- Close twoImgRow div -->
                     <% } %>
                 <% } else { %>
                     <div class="img1">
                      <a class="imagesLink1" href="/projects/<%= project._id %>">
                      <img src="<%=project.images[0].url %>" class="picture1" alt="" loading="lazy">
                      <div class="textAndButton1">
                        <h1 class="img1Text"><%= project.name %></h1>
                       <button class="img1Button">&rarr;</button>
                      </div>
                         </a>
                     </div>
                 <% x++;} %>
             <% } else { %>
                 <% if (x === 7) { %>
                    </section>
                     <button id="toggle-btn" class="AddMoreButton">Show more</button>
                     <section id="my-section" class="AddMoreSection">
                 <% } %>
                 <% if (x % 2 === 1) { %>
                     <div class="twoImgRow">
                       
                 <% } %>
                 <div class="img2">
                  <a class="imagesLink" href="/projects/<%= project._id %>">
                    <img class="picture" src="<%=project.images[0].url %>" alt="" loading="lazy">
                    <div class="textAndButton">
                     <h1 class="img2Text"><%= project.name %></h1>
                     <button class="img2Button">&rarr;</button>
                     </div>
                    </a>
                 </div>
                 <% if (x % 2 === 0 ) { %>
                     </div> <!-- Close twoImgRow div -->
                   
                 <% } %>
             <% } %>
             <% x++; %>
         <% } %>
     </section>
     <section class="ReviewsSection">
              <h2 class="ReviewsSectionH2">Client reviews</h2>
              <h3 class="ReviewsSectionRatings"><span class="ReviewsSectionRatingsSpansN">Overall rating:</span> <span class="ReviewsSectionRatingsSpans"><%=averageRating%> ⭐️</span><span class="ReviewsSectionRatingsSpansN"> based on</span> <span class="ReviewsSectionRatingsSpans"><%=totalReviews%></span> <span class="ReviewsSectionRatingsSpansN">reviews</span></h3>
        <div class="review-carousel-div">
        <% review.forEach((r, i) => { %>
            <div class="review-carousel <%= i === 0 ? 'active' : '' %>">
              <div class="review-carousel-stars">
               <% for (let s = 0; s < r.starRating; s++) { %>
                    <span class="star">⭐️</span>
                <% } %>
                </div>
                <h1 class="review-carousel-H1"><%= r.name %></h1>
                <p class="review-carousel-p"><%= r.message %></p>
            </div>
        <% }); %>
       
